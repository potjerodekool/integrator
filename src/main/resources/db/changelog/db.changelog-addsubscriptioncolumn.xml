<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="add-feedentry_category table" author="Evert Tigchelaar">
        <addColumn tableName="feedentry">
            <column name="subscription_id" type="INTEGER">
                <constraints nullable="false"
                             foreignKeyName="feedentry'_sync_feed_subscription"
                             referencedTableName="synd_feed_subscription"
                             referencedColumnNames="id"
                />
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>